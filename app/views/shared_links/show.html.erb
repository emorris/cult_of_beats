<div class="navbar bg-base-100">
  <div class="navbar-start">
   <a href="/" class="btn btn-ghost normal-case text-xl">cultOfBeats</a>
  </div>
  <div class="navbar-center">
    <a class="btn btn-ghost normal-case text-xl"><%= @user_profile.name %>'s Links</a>
  </div>
  <div class="navbar-end">
  </div>
</div>

<div class="md:container md:mx-auto">
  <%  @user_profile.site_links.map do |key, url| %>
    <div class="grid gap-4 grid-cols-1 justify-items-center">
      <a href="<%= url %>" target="_blank" class="btn btn-block w-96 ">
        <i class="fa fa-brands fa-2x fa-<%= key %> mr-2"></i>
        <%= key %>
      </button>
    </a>
  <% end %>
  <%  @user_profile.shared_contents.each do |shared_content| %>

  <div class="card w-96 bg-neutral shadow-xl">
    <figure> <%=  shared_content.html_embed.html_safe %></figure>
    <div class="card-body items-center text-center">
      <h2 class="card-title"><%= shared_content.title %></h2>
    </div>
  </div>
   
  <% end %>
</div>